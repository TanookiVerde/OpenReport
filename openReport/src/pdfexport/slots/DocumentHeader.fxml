<TitledPane expanded="false" stylesheets="@../../front/scene_slot.css" text="Cabeçalho (Documento)" textFill="#1a7f86" xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pdfexport.slots.DocumentHeader">
   <content>
      <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="670.0" prefWidth="200.0">
